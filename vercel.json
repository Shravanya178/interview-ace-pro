{
  "buildCommand": "node vercel-build.js && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@1.5.8"
    }
  },
  "public": false
} 